{
  "kind": "collectionType",
  "collectionName": "amer_services",
  "info": {
    "singularName": "amer-service",
    "pluralName": "amer-services",
    "displayName": "amerService",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "serviceName": {
      "type": "string"
    },
    "applicantName": {
      "type": "string"
    },
    "sponsorName": {
      "type": "string"
    },
    "sponsorEmail": {
      "type": "string"
    },
    "sponsorMobileNo": {
      "type": "string"
    },
    "whatAppNo": {
      "type": "string"
    },
    "acount_IBAN_No": {
      "type": "string"
    },
    "applicationPriority": {
      "type": "string"
    },
    "insideOrOutside": {
      "type": "string"
    },
    "requiredDocument": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "address": {
      "type": "text"
    },
    "comment": {
      "type": "text"
    },
    "Status": {
      "type": "enumeration",
      "enum": [
        "New Request",
        "Verified",
        "Sent to Typist",
        "Payment Link Requested",
        "Submitted"
      ],
      "default": "New Request"
    },
    "Note_to_Customer": {
      "type": "text"
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "admin::user"
    }
  }
}
